[  {    "type": "post",    "url": "/login",    "title": "User login route",    "group": "Users",    "name": "loginUser",    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "String",            "optional": true,            "field": "email",            "description": "<p>User must provide valid email address.</p>"          },          {            "group": "Parameter",            "type": "String",            "optional": true,            "field": "password",            "description": "<p>User must provide password.</p>"          }        ]      },      "examples": [        {          "title": "Request Params:",          "content": "{\n\"email\": 'dayan@gmail.com',\n\"password\":\"passwordD#$\"\n}",          "type": "String"        }      ]    },    "success": {      "fields": {        "Success 200": [          {            "group": "Success 200",            "type": "Object",            "optional": false,            "field": "Token",            "description": "<p>A Json Web Token generated to access protected routed</p>"          }        ]      },      "examples": [        {          "title": "Signup -Success-Response:",          "content": "HTTP/1.1 200 OK\n{\n\"token\":\"eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9\"\n}",          "type": "json"        }      ]    },    "examples": [      {        "title": "Example usage:",        "content": "curl -i http://localhost:8000/login",        "type": "curl"      }    ],    "description": "<p>User can login with valid email and password</p>",    "version": "0.0.0",    "filename": "controllers/user-controller.js",    "groupTitle": "Users"  },  {    "type": "post",    "url": "/signup",    "title": "Register User information",    "group": "Users",    "name": "signupUser",    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "String",            "optional": true,            "field": "email",            "description": "<p>User must provide valid email address.</p>"          },          {            "group": "Parameter",            "type": "String",            "optional": true,            "field": "password",            "description": "<p>User must provide password.</p>"          }        ]      },      "examples": [        {          "title": "Request Params:",          "content": "{\n\"email\": 'dayan@gmail.com',\n\"password\":\"passwordD#$\"\n}",          "type": "String"        }      ]    },    "success": {      "fields": {        "Success 200": [          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "User",            "description": "<p>signup successful.</p>"          }        ]      },      "examples": [        {          "title": "Signup -Success-Response:",          "content": "HTTP/1.1 200 OK\n{\n\"msg\":\"User signup successful\"\n}",          "type": "json"        }      ]    },    "examples": [      {        "title": "Example usage:",        "content": "curl -i http://localhost:8000/signup",        "type": "curl"      }    ],    "description": "<p>User can register or create a new account</p>",    "version": "0.0.0",    "filename": "controllers/user-controller.js",    "groupTitle": "Users"  }]
